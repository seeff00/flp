<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>

{% include('header.html.twig') %}

{% if app.request.pathinfo != '/admin' and app.request.pathinfo != '/login' and app.request.pathinfo != '/register' %}
    {% include('navbar.html.twig') %}
{% endif %}

{% block body %}{% endblock %}

{% include('footer.html.twig') %}

<script src="https://www.google.com/recaptcha/api.js?render=6LdX9CcpAAAAAHsbVpU56Az2rSTDJf4q7oCvys3z"></script>
</body>
</html>
